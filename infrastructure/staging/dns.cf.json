{
  "Parameters": {
    "domainName": {
      "Type": "String",
      "Default": "stage.jnlapp.io"
    }
  },
  "Resources": {
    "stagingHostedZone": {
      "Type": "AWS::Route53::HostedZone",
      "Properties": {
        "Name": { "Ref": "domainName" }
      },
      "DeletionPolicy": "Delete"
    }
  }
}
